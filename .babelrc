{
  "env": {
    "development": {
      "presets": [
        "env", "react"
      ],
      "plugins": [
        "transform-import-bangtext-node",
        [
          "transform-runtime", {
            "polyfill": false,
            "regenerator": true
          }
        ]
      ]
    },
    "client": {
      "presets": [
        ["react", "env", {"es2015": {"modules": false}}]
      ],
      "plugins": [
        "external-helpers"
      ]
    }
  }
}
